MDScreen:
    text_input: text_input
    chat2: chat2
    #copy_txt: copy_txt
    copy_txt: copy_txt
    send: send
    name: "assistchat7"
    MDFloatLayout:
        md_bg_color: .1, .1, .1, 1
            
        MDIconButton:
            icon: "arrow-left"
            pos_hint: {"center_y": .95}
            user_font_size: "20sp"
            theme_text_color: "Custom"
            text_color: rgba(71, 92, 119, 255)
            on_release:
                root.manager.transition.direction = "right"
                root.manager.current = "assistant"
                
             
            
        MDIconButton:
            icon: "help-circle-outline"
            pos_hint: {"center_x": .9, "center_y": .95}
            #pos_hint: {"center_y": .95}
            user_font_size: "20sp"
            theme_text_color: "Custom"
            text_color: rgba(71, 92, 119, 255)
            on_release:
                root.manager.transition.direction = "left"
                root.manager.current = "about_ass7"
                
        MDFloatLayout:
            MDFloatLayout:
                size_hint_y: .06
                pos_hint: {"center_y": .95}
                MDLabel:
                    id: mode
                    text: "All About Islamic Religion"

                    pos_hint: {"center_y": .5}
                    halign: "center"
                    font_name: "RSan.ttf"
                    font_size: "15sp"
                    theme_text_color: "Custom"
                    color: rgba(211, 211, 211, 255)
                    halign: "center"

            MDFloatLayout:
              #  id: copy_txt
                ProfileCard:
                    size_hint: .9, .1
                    pos_hint: {"center_x":.5, "center_y": .88}
                    radius: [0, 0, 16, 16]
                    MDFloatLayout:
                        size_hint: .15, .8
                        pos_hint: {"center_x": .1, "center_y": .50}
                        canvas.before:
                            Color:
                                rgb: 1, 1, 1, 1
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [40]
                                source: "img7.png"

                    MDLabel:
                        text: "Ali"
                        font_size: "15sp"
                        font_name: "RSan.ttf"
                        pos_hint: {"center_x": .72, "center_y": .6}
                        color: rgba(135, 200, 133, 255)
                    MDLabel:
                        text:  "Islamic Scholar"
                        font_size: "11sp"
                        font_name: "RSan.ttf"
                        pos_hint: {"center_x": .72, "center_y": .32}
                        color: rgba(188, 202, 228, 255)
                    MDIconButton:
                        id: copy_txt
                        icon: "content-copy"
                        pos_hint: {"center_x": .93, "center_y": .5}
                        user_font_size: "20sp"
                        theme_text_color: "Custom"
                        text_color: rgba(51, 51, 51, 255)
                        disable: True
                        on_release: app.copy_press()
